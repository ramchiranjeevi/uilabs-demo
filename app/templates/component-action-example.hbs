{{#ui-dialog success=(action 'success') cancel=(action 'cancel') statusObject=object as | updateObject |}}
    {{ui-dialog-content
        valueOne=updateObject.textOne
        statusMessageOne=updateObject.statusMessageOne
        statusClassOne=updateObject.statusClassOne
        valueTwo=updateObject.textTwo
        statusMessageTwo=updateObject.statusMessageTwo
        statusClassTwo=updateObject.statusClassTwo
    }}
{{/ui-dialog}}
